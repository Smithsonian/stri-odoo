<?xml version='1.0' encoding='utf-8'?>
<odoo>
	<data>
		<!-- Add you code here -->
		
		<!-- model.name inherit form view -->
		<record id="invoice_report_inherit" model="ir.ui.view">
			<field name="name">invoice.report.inherit</field>
			<field name="model">account.invoice.report</field>
			<field name="inherit_id" ref="account.view_account_invoice_report_graph"/>
			<field name="arch" type="xml">
			<graph string="Invoices Analysis">
                <xpath expr="//field[@name='categ_id']" position="before">
					<field name="property_account_income_id"/>
					<field name="stri_chartfield"/>
				</xpath>
             </graph>
         
				
			</field>
		</record>

	</data>
</odoo>