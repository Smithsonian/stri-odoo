<?xml version='1.0' encoding='utf-8'?>
<odoo>
	<data>
		
        <record id = "vso_form_view" model = "ir.ui.view">
            <field name = "name">VSO View Form</field>
            <field name = "model">muki.rest</field>
            <field name = "arch" type = "xml">
                <form string = "VSO Search" create="false" edit="false" delete="false">
					<header>
						<button name="button_search" string="Search" class="oe_highlight" type="object"/>
					</header>
                	<sheet> 
                        <group>
                            <group>
                            	<field name = "name" />
								<field name = "hstatus" />
                            </group>
						</group>
					</sheet>
                </form>
            </field>
        </record>

		<record id="muki_rest_action" model="ir.actions.act_window">
			<field name="name">TEST VSO</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">muki.rest</field>
			<field name="view_mode">tree,form</field>
			<field name="view_type">form</field>
			
		</record>

		<menuitem id="muki_rest_menu" name="Test VSO" parent="account.menu_finance_receivables" action="muki_rest_action" sequence=""/>
		
	</data>
</odoo>