<?xml version='1.0' encoding='utf-8'?>
<odoo>
	<data>
		
		<template id="account_reports.filter_info_template">
			<xpath expr="//div[@class='print_only']//div[0]" position="after">
				<div class="col-12">
					<t t-if="options.get('category') != None">
						Category:
						<t t-set="category_value" t-value="'All'"/>
						<t t-foreach="options['category']" t-as="j">
							<t t-if="j.get('selected') == True">
								<t t-set="category_value" t-value="''"/>
								<t t-esc="j.get('value')"/>
							</t>
						</t>
						<t t-esc="category_value"/>
					</t>
				</div>
			</xpath>
		</template>
	</data>
</odoo>